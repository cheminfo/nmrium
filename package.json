{
  "name": "nmrium",
  "description": "React component to display and process nuclear magnetic resonance (NMR) spectra.",
  "version": "0.18.3",
  "license": "MIT",
  "main": "lib-cjs/component/NMRium.js",
  "module": "lib/component/NMRium.js",
  "types": "lib/component/NMRium.d.ts",
  "files": [
    "lib",
    "lib-cjs"
  ],
  "scripts": {
    "build": "vite build --outDir build",
    "build-no-minify": "cross-env NO_MINIFY=true npm run build",
    "build-with-profiling": "cross-env WITH_PROFILING=true npm run build",
    "check-types": "tsc --noEmit",
    "compile": "tsc --project tsconfig.esm.json && tsc --project tsconfig.cjs.json",
    "clean-lib": "rimraf lib lib-cjs",
    "dev": "vite --host localhost --strictPort --open",
    "eslint": "eslint . --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "prepack": "npm run clean-lib && npm run compile",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "start": "npm run dev",
    "test": "npm run test-only && npm run build-no-minify && npm run test-e2e && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "jest --coverage",
    "test-e2e": "playwright test",
    "test-e2e-server": "serve -l tcp://localhost:3000 build"
  },
  "bugs": {
    "url": "https://github.com/cheminfo/nmrium/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/nmrium.git"
  },
  "volta": {
    "node": "16.9.1"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "analysis-ui-components": "^0.6.1",
    "cheminfo-font": "^1.8.1",
    "d3": "^7.0.3",
    "eventemitter3": "^4.0.7",
    "file-saver": "^2.0.5",
    "formik": "^2.2.9",
    "immer": "^9.0.6",
    "jszip": "^3.7.1",
    "lodash": "^4.17.21",
    "mf-parser": "^1.3.4",
    "ml-airpls": "^1.0.0",
    "ml-array-max": "^1.2.3",
    "ml-array-median": "^1.1.5",
    "ml-array-xy-equally-spaced": "^1.2.0",
    "ml-baseline-correction-regression": "^1.0.0",
    "ml-conrec": "^3.2.1",
    "ml-fft": "^1.3.5",
    "ml-gsd": "^6.7.0",
    "ml-regression-polynomial": "^2.2.0",
    "ml-spectra-processing": "^6.8.0",
    "ml-stat": "^1.3.3",
    "multiplet-analysis": "^2.0.0",
    "nmr-correlation": "^2.1.0",
    "nmr-parser": "^1.7.0",
    "nmr-processing": "^2.5.0",
    "nmredata": "^0.5.0",
    "numeral": "^2.0.6",
    "openchemlib": "^7.4.2",
    "openchemlib-utils": "^1.5.1",
    "re-resizable": "^6.9.1",
    "react-card-flip": "^1.1.3",
    "react-color": "2.17.2",
    "react-d3-utils": "^0.3.0",
    "react-draggable": "^4.4.4",
    "react-dropzone": "^11.4.0",
    "react-error-boundary": "^3.1.3",
    "react-icons": "^4.2.0",
    "react-inspector": "^5.1.1",
    "react-mf": "^2.0.1",
    "react-ocl": "^4.3.3",
    "react-ocl-nmr": "^1.1.0",
    "react-plot": "^0.10.1",
    "react-rnd": "^10.3.5",
    "react-slider": "^1.3.1",
    "react-table": "7.6.0",
    "react-transition-group": "^4.4.2",
    "react-use": "^17.3.1",
    "spectrum-generator": "^5.1.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@playwright/test": "^1.14.1",
    "@simbathesailor/use-what-changed": "^2.0.0",
    "@types/d3": "^7.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.4",
    "@types/react": "^17.0.22",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.9",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@zakodium/eslint-config": "^3.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "jest": "^27.2.1",
    "modern-normalize": "^1.1.0",
    "prettier": "^2.4.1",
    "rc-menu": "^9.0.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "reactstrap": "^8.10.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "serve": "^12.0.1",
    "typescript": "~4.4.3",
    "vite": "^2.5.10"
  }
}
