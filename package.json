{
  "name": "nmrium",
  "description": "React component to display and process nuclear magnetic resonance (NMR) spectra.",
  "version": "0.14.0",
  "license": "MIT",
  "main": "lib-cjs/component/NMRium.js",
  "module": "lib/component/NMRium.js",
  "types": "lib/component/NMRium.d.ts",
  "files": [
    "lib",
    "lib-cjs"
  ],
  "scripts": {
    "build": "vite build --outDir build",
    "build-with-profiling": "npm run build -- --mode profiling --sourcemap",
    "check-types": "tsc --noEmit",
    "compile": "tsc --project tsconfig.esm.json && tsc --project tsconfig.cjs.json",
    "clean-lib": "rimraf lib lib-cjs",
    "dev": "vite --open",
    "eslint": "eslint . --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "prepack": "npm run clean-lib && npm run compile",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "start": "npm run dev",
    "test": "npm run test-only && npm run build && npm run test-e2e && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "jest --coverage",
    "test-e2e": "start-server-and-test test-e2e-server http://localhost:3000 test-e2e-jest",
    "test-e2e-jest": "cd test-e2e && jest",
    "test-e2e-server": "serve -l 3000 build"
  },
  "bugs": {
    "url": "https://github.com/cheminfo/nmrium/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/nmrium.git"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "cheminfo-font": "^1.6.0",
    "d3": "^6.7.0",
    "eventemitter3": "^4.0.7",
    "file-saver": "^2.0.5",
    "formik": "^2.2.8",
    "immer": "^9.0.2",
    "jszip": "^3.6.0",
    "lodash": "^4.17.21",
    "mf-parser": "^1.2.0",
    "ml-airpls": "^1.0.0",
    "ml-array-max": "^1.2.3",
    "ml-array-median": "^1.1.5",
    "ml-array-xy-equally-spaced": "^1.2.0",
    "ml-baseline-correction-regression": "^1.0.0",
    "ml-conrec": "^3.2.0",
    "ml-fft": "^1.3.5",
    "ml-gsd": "^6.6.2",
    "ml-regression-polynomial": "^2.1.0",
    "ml-spectra-processing": "^6.6.0",
    "ml-stat": "^1.3.3",
    "multiplet-analysis": "^1.0.1",
    "nmr-correlation": "^1.3.2",
    "nmr-parser": "^1.4.0",
    "nmr-processing": "^1.4.0",
    "nmredata": "^0.4.0",
    "numeral": "^2.0.6",
    "openchemlib": "^7.4.0",
    "openchemlib-utils": "^1.2.0",
    "prop-types": "^15.7.2",
    "re-resizable": "^6.9.0",
    "react-card-flip": "^1.1.1",
    "react-color": "2.17.2",
    "react-draggable": "^4.4.3",
    "react-dropzone": "^11.3.2",
    "react-error-boundary": "^3.1.3",
    "react-icons": "^4.2.0",
    "react-inspector": "^5.1.1",
    "react-mf": "^2.0.1",
    "react-ocl": "^4.3.3",
    "react-ocl-nmr": "^1.0.0",
    "react-plot": "^0.10.0",
    "react-rnd": "^10.3.0",
    "react-slider": "^1.1.4",
    "react-split-pane": "^0.1.92",
    "react-table": "7.6.0",
    "react-transition-group": "^4.4.1",
    "react-use": "^17.2.4",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/d3": "^6.7.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "@zakodium/eslint-config": "^3.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.27.0",
    "jest": "^27.0.1",
    "modern-normalize": "^1.1.0",
    "playwright": "^1.11.1",
    "prettier": "^2.3.0",
    "rc-menu": "^9.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.9.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "serve": "^11.3.2",
    "start-server-and-test": "^1.12.3",
    "typescript": "~4.3.2",
    "vite": "^2.3.4"
  },
  "volta": {
    "node": "16.2.0"
  }
}
